  <!--Main layout-->
  <main>
    <div class="container">

      <!--Navbar-->
      <nav class="navbar navbar-expand-lg navbar-dark mdb-color lighten-3 mt-3 mb-5">

        <!-- Navbar brand -->
        <!-- <span class="navbar-brand">Categories:</span> -->

        <!-- Collapse button -->
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#basicExampleNav"
          aria-controls="basicExampleNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>

        <!-- Collapsible content -->
        <div class="collapse navbar-collapse" id="basicExampleNav">

          <!-- Links -->
          <ul class="navbar-nav mr-auto">
            <li class="nav-item active">
              <a class="nav-link" href="/stocks">All</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/stocks/Body-Accessories">Body Accessories</a>

            </li>
            <li class="nav-item">
              <a class="nav-link" href="/stocks/Electronics">Electronics</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/stocks/Clothings">Clothings</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/stocks/Food">Food<i class="fas fa-search"></i></a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/stocks/Services">Services</a>
            </li>

          </ul>
          <!-- Links -->

          <form class="form-inline"  action="/search">
            <div class="md-form my-0">
              <span><i class="fas fa-search ml-2"></i></span>
              <input class="form-control mr-sm-2" name="search" type="text" placeholder="Search" aria-label="Search">
            </div>
          </form>
        </div>
        <!-- Collapsible content -->

      </nav>
      <!--/.Navbar-->

      <!--Section: Products v.3-->
      <section class="text-center mb-4">

        <!--Grid row-->
        <div class="row">
          {{# each products }}
           {{# each this }}
          <!--Grid column-->
          <div class="col-lg-3 col-md-6 mb-4">
           
            <!--Card-->
            <div class="card">

              <!--Card image-->
              <div class="view overlay">
                <img src="{{this.productImage}}" class="card-img-top"
                  alt="{{this.productName}}">
                  <!-- https://mdbootstrap.com/img/Photos/Horizontal/E-commerce/Vertical/12.jpg -->
                <a>
                  <div class="mask rgba-white-slight"></div>
                </a>

              </div>
              <!--Card image-->

              <!--Card content-->
              <div class="card-body text-center">
                <!--Category & Title-->
                <a href="" class="grey-text">
                  <h5>{{ this.productName }}</h5>
                </a>
                <h5>
                  <strong>
                    <a href="" class="dark-grey-text"><strong>{{ this.productName }}</strong>
                      <span class="badge badge-pill danger-color">{{ this.productStatus }}</span>
                    </a>
                  </strong>
                </h5>

                <h4 class="font-weight-bold blue-text">
                  <!-- <strong>${{this.productPrice}}</strong> -->
                </h4>
                <a href="/store/{{this.email}}/{{this._id}}">Link</a>
              </div>
              <!--Card content-->

            </div>
            <!--Card-->
             
          </div>
          {{/each}}
          <!--Grid column-->
           {{/each}}
        </div>
        <!--Grid row-->
      </section>
      <!--Section: Products v.3-->








      <!--Pagination-->
      <nav class="d-flex justify-content-center wow fadeIn">
